      "source": [
        "from jax import random\n",
        "\n",
        "import jax\n",
        "import jax.numpy as jnp\n",
        "import numpy as np\n",
        "\n",
        "key = random.PRNGKey(0)\n",
        "cov = np.array([[1.2, 0.4], [0.4, 1.0]])\n",
        "mean = np.array([3,-1])\n",
        "x1,x2 = jax.random.multivariate_normal(key, mean, cov, (5000,)).T"
      ]
    }
